# Supabase Configuration
# Supabase Dashboard > Settings > API에서 확인 가능
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_KEY=your_supabase_service_key_here

# Fund Configuration
NEXT_PUBLIC_FUND_NAME=프로펠-SNUSV엔젤투자조합2호

# OAuth Provider Configuration
# Kakao 로그아웃에 필요
NEXT_PUBLIC_KAKAO_REST_API_KEY=your_kakao_rest_api_key_here
# 로그아웃 후 리다이렉트될 URL (기본값: {origin}/login)
NEXT_PUBLIC_KAKAO_LOGOUT_REDIRECT_URI=http://localhost:3000/login

# Admin Configuration
# 관리자 이메일 목록 (쉼표로 구분)
NEXT_PUBLIC_ADMIN_EMAILS=admin@snusv.com,manager@snusv.com